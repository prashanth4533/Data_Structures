def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i
    return -1 

my_list = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
target = 5
result = linear_search(my_list, target)

if result != -1:
    print(f"Element found at index {result}")
else:
    print("Element not found in the list")
